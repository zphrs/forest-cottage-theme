    "errorForeground": "#F58F53",
    // "focusBorder": "#193632",
    "activityBar.background": "#040421",
    "activityBar.activeBorder": "#A8A9F4",
    "activityBar.foreground": "#CFD3FF",
    "activityBar.border": "#19363288",
    "widget.shadow": "#000000",
    "disabledForeground": "#535555",
    "descriptionForeground": "#E9EAE4",
    "widget.border": "#345348",
    "sash.hoverBorder": "#193632",
    "textLink.foreground": "#A8A9F4",
    "textLink.activeForeground": "#CFD3FF",
    "textBlockQuote.background": "#040421",
    "textCodeBlock.background": "#040421",
    "focusBorder": "#A8A9F4",
    "settings.modifiedItemIndicator": "#FFD140",

    "button.background": "#a8a9f4",
    "button.foreground": "#040421",
    "button.secondaryBackground": "#2D27A4",
    "button.secondaryForeground": "#A8A9F4",
    "button.hoverBackground": "#645cee",

    "editorGutter.background": "#040421",
    "editorLineNumber.foreground": "#535555",
    "editorLineNumber.activeForeground": "#cdc3ff",
    "editor.selectionBackground": "#cdc3ff40",
    "editorError.foreground": "#CE5509",
    "editorWarning.foreground": "#CE9409",

    "tab.activeBackground": "#040421",
    "tab.inactiveBackground": "#051e1a",
    "tab.inactiveForeground": "#939594",
    "tab.border": "#193632",
    "tab.hoverBackground": "#040421",
    "tab.activeForeground": "#cfd3ff",
    "tab.activeBorderTop": "#A8A9F4",
    "gitDecoration.untrackedResourceForeground": "#ACE9C5",
    "gitDecoration.modifiedResourceForeground": "#FFD140",
    "gitDecoration.deletedResourceForeground": "#c4977d",
    "list.errorForeground": "#F58F53",
    "list.warningForeground": "#CE9409",

    "editorGroupHeader.tabsBackground": "#193632",
    "titleBar.activeBackground": "#193632",
    "titleBar.activeForeground": "#cdc3ff",

    "input.background": "#040421",
    "inputOption.activeBorder": "#ace9c5",
    "input.foreground": "#e9eae4",
    "input.border": "#345348",
    "input.placeholderForeground": "#7e807f",
    "quickInput.background": "#193632",
    "quickInput.foreground": "#ace9c5",
    "quickInputTitle.background": "#040421",

    "editorWidget.resizeBorder": "#345348",

    "sideBar.background": "#040421",

    "list.focusBackground": "#2D27A440",
    "list.hoverBackground": "#2D27A440",
    "list.inactiveSelectionBackground": "#2D27A480",
    "list.activeSelectionBackground": "#2D27A4",

    "panel.background": "#040421",
    "panel.border": "#345348",

    "sideBar.border": "#345348",

    "statusBar.background": "#2d27a4",
    "statusBar.foreground": "#e9eae4",
    "statusBarItem.activeBackground": "#040421",
    "statusBarItem.hoverBackground": "#040421",

    "notifications.background": "#040421",
    "notifications.border": "#345348",
    "notificationCenter.border": "#345348"
  },
  "semanticHighlighting": false,
  "tokenColors": [
    {
      "name": "sea700",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ace9c5"
      }
    },
    {
      "name": "violet900",
      "scope": [
        "meta.tag.attributes.value",
        "variable",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#cfd3ff"
      }
    },
    {
      "name": "violet700",
      "scope": ["support.type.primitive", "entity", "variable.other.property"],
      "settings": {
        "foreground": "#a8a9f4"
      }
    },
    {
      "name": "gold700",
      "scope": [
        "string",
        "meta.property-value.css",
        "entity.other.attribute-name.class.css",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ffd140"
      }
    },
    {
      "name": "gold500",
      "scope": ["constant", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#CE9409"
      }
    },
    {
      "name": "gold900",
      "scope": [
        "keyword.operator",
        "keyword.control",
        "storage",
        "variable.language"
      ],
      "settings": {
        "foreground": "#fff4ba"
      }
    },
    {
      "name": "sea900",
      "scope": ["entity.name.function", "source.python"],
      "settings": {
        "foreground": "#d9ffe8"
      }
    },
    {
      "name": "gray990",
      "scope": ["source", "meta.brace"],
      "settings": {
        "foreground": "#e9eae4"
      }
    },
    {
      "name": "sea500",
      "scope": ["comment"],
      "settings": {
        "foreground": "#6e9d87"
      }
    }
  ],
  "type": "dark",
  "semanticTokenColors": {}
}
